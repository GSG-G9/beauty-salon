
{
    "env": {
      "commonjs": true,
      "es2021": true,
      "node": true
    },
    "extends": ["airbnb-base", "prettier", "plugin:jest/recommended"],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "plugins": ["prettier", "jest"],
    "rules": {
      "import/no-unresolved": [
        2, 
        { "caseSensitive": false }
     ],
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true
        }
      ]
    }
  }